  "mui-crud": {
    "prefix": "mui-crud",
    "body": [
      "  const {",
      "    rawData,",
      "    // 叫plugins其实是已绑定的技能列表",
      "    data: plugins,",
      "    noMore,",
      "    isLoadingMore,",
      "    isFetching,",
      "    loadMore,",
      "    size,",
      "    mutate,",
      "    actions,",
