    "prefix": "next-update",
    "body": [
      "  const handleUpdate = (name: string, previous: string) => {",
      "    const id = categoryMap[previous];",
      "    if (!id) return;",
      "    actions.Update(id, { id, name, type });",
      "  };",
      ""
    ],
    "description": ""
  },
